{
  "name": "template-pc-vue2-elementui",
  "version": "0.0.1",
  "templateName": "template-pc-vue2-elementui",
  "templateVersion": "0.0.1",
  "private": true,
  "scripts": {
    "mock": "vue-cli-service serve --host mock.local.fe.cn --port 50010 --open",
    "dev": "vue-cli-service serve --host dev.local.fe.cn --port 50010 --open",
    "test": "vue-cli-service serve --host test.local.fe.cn --port 50010 --open",
    "sit": "vue-cli-service serve --host sit.local.fe.cn --port 50010 --open",
    "uat": "vue-cli-service serve --host uat.local.fe.cn --port 50010 --open",
    "pre": "vue-cli-service serve --host pre.local.fe.cn --port 50010 --open",
    "fat": "vue-cli-service serve --host fat.local.fe.cn --port 50010 --open",
    "build": "vue-cli-service build",
    "build:analyse": "cross-env analyse=true  vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "dll": "webpack -p --progress --config ./webpack.dll.conf.js",
    "prettier": "prettier -c --write plop-templates/**/*.js src/**/*.js src/**/*.vue",
    "prepare": "husky install",
    "log": "standard-changelog -f && git add CHANGELOG.md",
    "commit": "commit"
  },
  "dependencies": {
    "core-js": "^3.15.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "axios": "^0.21.1",
    "compression-webpack-plugin": "^2.0.0",
    "element-ui": "2.13.0",
    "normalize.css": "7.0.0",
    "vue-axios": "^3.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/prompt-cli": "^12.1.4",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.1",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.3.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "dllPath": "public/vendor"
}
